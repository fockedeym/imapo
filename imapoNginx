server {
       listen 80;
       listen [::]:80;

       server_name imapo;

       root /var/www/imapoAngular;
       index index.html;

        location /api {
        proxy_set_header   X-Forwarded-For $remote_addr;
        proxy_set_header   Host $http_host;
        proxy_pass         "http://127.0.0.1:5000";
       }
       location / {
               try_files $uri $uri/ =404;
       }
}
